# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# TURN Server Configuration (Twilio recommended - lower cost)
TURN_PROVIDER=twilio
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token

# Alternative: Xirsys TURN
# XIRSYS_IDENT=your-ident
# XIRSYS_SECRET=your-secret
# XIRSYS_CHANNEL=your-channel

# Security
JWT_SECRET=your-jwt-secret-for-session-tokens
PIN_SALT=your-pin-salt-for-hashing

# Agent Configuration
AGENT_UPDATE_CHECK_INTERVAL=3600
AGENT_HEARTBEAT_INTERVAL=30

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MIN=100

# Environment
NODE_ENV=development
